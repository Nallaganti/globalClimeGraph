{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "gcg-analysis"
		},
		"ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS'"
		},
		"HttpServergit_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'HttpServergit'"
		},
		"ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gcganalysisdatalake.dfs.core.windows.net/"
		},
		"HttpServergit_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Nallaganti/globalClimeGraph/refs/heads/main/data/global-climate-change(1900-2023).txt"
		},
		"HttpServergit_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "anu04223@gmail.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "gcg-analysis_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_accountKey": {
						"value": "[parameters('ADLS_accountKey')]"
					},
					"HttpServergit_password": {
						"value": "[parameters('HttpServergit_password')]"
					},
					"ADLS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_properties_typeProperties_url')]"
					},
					"HttpServergit_properties_typeProperties_url": {
						"value": "[parameters('HttpServergit_properties_typeProperties_url')]"
					},
					"HttpServergit_properties_typeProperties_userName": {
						"value": "[parameters('HttpServergit_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}